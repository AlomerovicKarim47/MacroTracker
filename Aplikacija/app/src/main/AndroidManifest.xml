            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DodajActivity"></activity>
        <activity android:name=".ZabiljeziKolicinaActivity"></activity>
        <activity android:name=".ZabiljeziListaActivity"></activity>
        </application>

</manifest>